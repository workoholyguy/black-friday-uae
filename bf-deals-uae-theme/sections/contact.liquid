{% doc %}
  Contact page section with business details + contact form.

  Goals:
  - Clean Black Friday look (black / white / red)
  - Clear hierarchy (intro → ways to reach us → FAQs → form)
  - Mobile-first, no “random borders everywhere”
{% enddoc %}

<section class="contact full-width" data-section-id="{{ section.id }}">
  <div class="contact__inner">

    <!-- HERO / INTRO -->
    <header class="contact__hero" aria-label="{{ 'contact.heading' | t }}">
      <div class="contact__hero-left">
        <p class="contact__eyebrow">{{ 'contact.eyebrow' | t }}</p>

        <h1 class="contact__heading">
          {% if section.settings.heading != blank %}
            {{ section.settings.heading | escape }}
          {% else %}
            {{ 'contact.heading' | t }}
          {% endif %}
        </h1>

        <p class="contact__subheading">
          {% if section.settings.subheading != blank %}
            {{ section.settings.subheading | escape }}
          {% else %}
            {{ 'contact.subheading' | t }}
          {% endif %}
        </p>

        <div class="contact__hero-copy">
          <p>{{ 'contact.intro_1' | t }}</p>
          <p>{{ 'contact.intro_2' | t }}</p>
        </div>

        <div class="contact__stats" role="list">
          <article class="contact__stat" role="listitem">
            <p class="contact__stat-value">98%</p>
            <p class="contact__stat-label">{{ 'contact.stats_fast' | t }}</p>
          </article>
          <article class="contact__stat" role="listitem">
            <p class="contact__stat-value">24h</p>
            <p class="contact__stat-label">{{ 'contact.stats_support' | t }}</p>
          </article>
          <article class="contact__stat" role="listitem">
            <p class="contact__stat-value">+2k</p>
            <p class="contact__stat-label">{{ 'contact.stats_returns' | t }}</p>
          </article>
        </div>

        <div class="contact__quick-actions">
          {% assign phone_compact = section.settings.phone | default: '' | replace: ' ', '' | replace: '-', '' %}
          {% assign wa_number = section.settings.whatsapp | default: phone_compact | replace: '+', '' | replace: ' ', '' | replace: '-', '' %}

          {% if phone_compact != blank %}
            <a class="contact__btn contact__btn--primary" href="tel:{{ phone_compact }}">
              {{ 'contact.action_call' | t }}
            </a>
          {% endif %}

          {% if wa_number != blank %}
            <a class="contact__btn contact__btn--ghost" href="https://wa.me/{{ wa_number }}" target="_blank" rel="noopener">
              {{ 'contact.action_whatsapp' | t }}
            </a>
          {% endif %}

          <a class="contact__btn contact__btn--ghost" href="{{ routes.search_url }}">
            {{ 'contact.action_search' | t }}
          </a>
        </div>
      </div>

      <aside class="contact__hero-right" aria-label="{{ 'contact.services_title' | t }}">
        <h2 class="contact__card-title">{{ 'contact.services_title' | t }}</h2>

        <div class="contact__services">
          <article class="contact__service">
            <h3 class="contact__service-title">{{ 'contact.service_returns' | t }}</h3>
            <p class="contact__service-copy">{{ 'contact.service_returns_copy' | t }}</p>
          </article>

          <article class="contact__service">
            <h3 class="contact__service-title">{{ 'contact.service_delivery' | t }}</h3>
            <p class="contact__service-copy">{{ 'contact.service_delivery_copy' | t }}</p>
          </article>

          <article class="contact__service">
            <h3 class="contact__service-title">{{ 'contact.service_support' | t }}</h3>
            <p class="contact__service-copy">{{ 'contact.service_support_copy' | t }}</p>
          </article>
        </div>

        <div class="contact__mini">
          <div class="contact__mini-row">
            <span class="contact__mini-label">{{ 'contact.hours_label' | t }}</span>
            <span class="contact__mini-value">
              {% if section.settings.hours != blank %}
                {{ section.settings.hours | escape }}
              {% else %}
                {{ 'contact.hours_fallback' | t }}
              {% endif %}
            </span>
          </div>

          {% if section.settings.email != blank %}
            <div class="contact__mini-row">
              <span class="contact__mini-label">{{ 'contact.email_label' | t }}</span>
              <span class="contact__mini-value">
                <a href="mailto:{{ section.settings.email | escape }}">{{ section.settings.email | escape }}</a>
              </span>
            </div>
          {% endif %}

          {% if section.settings.phone != blank %}
            <div class="contact__mini-row">
              <span class="contact__mini-label">{{ 'contact.phone_label' | t }}</span>
              <span class="contact__mini-value">
                <a href="tel:{{ phone_compact }}">{{ section.settings.phone | escape }}</a>
              </span>
            </div>
          {% endif %}
        </div>
      </aside>
    </header>

    <!-- MAIN CONTENT GRID -->
    <div class="contact__grid">
      <!-- LEFT: DETAILS + FAQ + MAP -->
      <div class="contact__details">

        <section class="contact__panel" aria-label="{{ 'contact.panel_reach_us_title' | t }}">
          <h2 class="contact__panel-title">{{ 'contact.panel_reach_us_title' | t }}</h2>
          <p class="contact__panel-subtitle">{{ 'contact.panel_reach_us_copy' | t }}</p>

          <div class="contact__extras">
            <article class="contact__extra">
              <span class="contact__icon" aria-hidden="true">
                <svg viewBox="0 0 24 24">
                  <path d="M12 2a4 4 0 0 1 4 4c0 3-4 9-4 9s-4-6-4-9a4 4 0 0 1 4-4zm0 6a2 2 0 1 0 0-4 2 2 0 0 0 0 4z"></path>
                </svg>
              </span>
              <div class="contact__extra-body">
                <h3 class="contact__extra-title">{{ 'contact.extra_pickup_title' | t }}</h3>
                <p class="contact__extra-copy">
                  {% if section.settings.address != blank %}
                    {{ section.settings.address | newline_to_br }}
                  {% else %}
                    {{ 'contact.address_fallback' | t }}
                  {% endif %}
                </p>
              </div>
            </article>

            <article class="contact__extra">
              <span class="contact__icon" aria-hidden="true">
                <svg viewBox="0 0 24 24">
                  <path d="M21 16.5c-1.2 0-2.4-.2-3.4-.6-.3-.1-.7 0-.9.2l-2.7 2c-3-1.5-5.4-3.8-7-7l2-2.7c.4-.4.5-.9.2-1.3-.4-1-1-2.2-1.8-3.3-.2-.3-.5-.4-.8-.4H3.5c-.3 0-.6.1-.8.4-.8.9-.9 2-.7 3.4.4 3 1.6 6.3 4.8 9.6 3.1 3 6.4 4.3 9.6 4.8 1.3.2 2.5.1 3.3-.7.3-.2.4-.5.4-.8V17c0-.3-.1-.5-.3-.7z"></path>
                </svg>
              </span>
              <div class="contact__extra-body">
                <h3 class="contact__extra-title">{{ 'contact.extra_call_title' | t }}</h3>
                <p class="contact__extra-copy">
                  {% if section.settings.phone != blank %}
                    <a href="tel:{{ phone_compact }}">{{ section.settings.phone | escape }}</a>
                  {% else %}
                    {{ 'contact.phone_fallback' | t }}
                  {% endif %}
                  <span class="contact__muted"> · {{ 'contact.availability' | t }}</span>
                </p>
              </div>
            </article>

            <article class="contact__extra">
              <span class="contact__icon" aria-hidden="true">
                <svg viewBox="0 0 24 24">
                  <path d="M3 5v14c0 1.1.9 2 2 2h14c1.1 0 2-.9 2-2V5c0-1.1-.9-2-2-2H5c-1.1 0-2 .9-2 2zm2 0h14v1.5L12 11 5 6.5V5zm0 3.1L12 12l7-3.9v10.9H5V8.1z"></path>
                </svg>
              </span>
              <div class="contact__extra-body">
                <h3 class="contact__extra-title">{{ 'contact.extra_email_title' | t }}</h3>
                <p class="contact__extra-copy">
                  {% if section.settings.email != blank %}
                    <a href="mailto:{{ section.settings.email | escape }}">{{ section.settings.email | escape }}</a>
                  {% else %}
                    {{ 'contact.email_fallback' | t }}
                  {% endif %}
                  <span class="contact__muted"> · {{ 'contact.reply_time' | t }}</span>
                </p>
              </div>
            </article>
          </div>
        </section>

        {% assign map_embed = section.settings.map_embed | strip %}
        {% if map_embed contains '<iframe' %}
          <section class="contact__panel" aria-label="{{ 'contact.map_title' | t }}">
            <div class="contact__panel-head">
              <h2 class="contact__panel-title">{{ 'contact.map_title' | t }}</h2>
              <p class="contact__panel-subtitle">{{ 'contact.map_copy' | t }}</p>
            </div>

            <div class="contact__map">
              {{ map_embed }}
            </div>
          </section>
        {% endif %}

        <section class="contact__panel" aria-label="{{ 'contact.faq_title' | t }}">
          <h2 class="contact__panel-title">{{ 'contact.faq_title' | t }}</h2>
          <p class="contact__panel-subtitle">{{ 'contact.faq_subtitle' | t }}</p>

          <div class="contact__faq" role="list">
            <details class="contact__faq-item" open>
              <summary>{{ 'contact.faq1_question' | t }}</summary>
              <div class="contact__faq-answer">
                <p>{{ 'contact.faq1_answer' | t }}</p>
              </div>
            </details>

            <details class="contact__faq-item">
              <summary>{{ 'contact.faq2_question' | t }}</summary>
              <div class="contact__faq-answer">
                <p>{{ 'contact.faq2_answer' | t }}</p>
              </div>
            </details>

            <details class="contact__faq-item">
              <summary>{{ 'contact.faq3_question' | t }}</summary>
              <div class="contact__faq-answer">
                <p>{{ 'contact.faq3_answer' | t }}</p>
              </div>
            </details>

            <details class="contact__faq-item">
              <summary>{{ 'contact.faq4_question' | t }}</summary>
              <div class="contact__faq-answer">
                <p>{{ 'contact.faq4_answer' | t }}</p>
              </div>
            </details>
          </div>
        </section>
      </div>

      <!-- RIGHT: FORM -->
      <aside class="contact__form" aria-label="{{ 'contact.form_title' | t }}">
        <div class="contact__panel contact__panel--sticky">
          <h2 class="contact__panel-title">{{ 'contact.form_title' | t }}</h2>
          <p class="contact__panel-subtitle">{{ 'contact.form_subtitle' | t }}</p>

          {% form 'contact', id: 'ContactForm' %}
            {% if form.posted_successfully? %}
              <p class="contact__success" role="status">{{ 'contact.success' | t }}</p>
            {% endif %}

            {% if form.errors %}
              <ul class="contact__errors" role="alert">
                {% for field in form.errors %}
                  <li>{{ form.errors.messages[field] }}</li>
                {% endfor %}
              </ul>
            {% endif %}

            <div class="field">
              <label for="ContactName">{{ 'contact.name' | t }}</label>
              <input
                id="ContactName"
                type="text"
                name="contact[name]"
                value="{{ form.name | default: customer.name }}"
                autocomplete="name"
                required
              >
            </div>

            <div class="field">
              <label for="ContactEmail">{{ 'contact.email' | t }}</label>
              <input
                id="ContactEmail"
                type="email"
                name="contact[email]"
                value="{{ form.email | default: customer.email }}"
                autocomplete="email"
                required
              >
            </div>

            <div class="field">
              <label for="ContactPhone">{{ 'contact.phone' | t }}</label>
              <input
                id="ContactPhone"
                type="tel"
                name="contact[phone]"
                value="{{ form.phone | default: customer.phone }}"
                autocomplete="tel"
              >
            </div>

            <div class="field">
              <label for="ContactOrder">{{ 'contact.order' | t }}</label>
              <input
                id="ContactOrder"
                type="text"
                name="contact[order_number]"
                placeholder="{{ 'contact.order_placeholder' | t }}"
              >
            </div>

            <div class="field">
              <label for="ContactMessage">{{ 'contact.message' | t }}</label>
              <textarea id="ContactMessage" name="contact[body]" rows="6" required>{{ form.body }}</textarea>
              <p class="contact__hint">{{ 'contact.message_hint' | t }}</p>
            </div>

            <button type="submit" class="contact__submit">
              {{ 'contact.submit' | t }}
            </button>
          {% endform %}

          <div class="contact__cta" aria-label="{{ 'contact.cta_title' | t }}">
            <div>
              <p class="contact__cta-title">
                {% if section.settings.cta_heading != blank %}
                  {{ section.settings.cta_heading | escape }}
                {% else %}
                  {{ 'contact.cta_title' | t }}
                {% endif %}
              </p>
              <p class="contact__cta-subtitle">
                {% if section.settings.cta_description != blank %}
                  {{ section.settings.cta_description | escape }}
                {% else %}
                  {{ 'contact.cta_detail' | t }}
                {% endif %}
              </p>
            </div>
            <a class="contact__cta-btn" href="{{ section.settings.cta_link | default: routes.cart_url }}">
              {% if section.settings.cta_button != blank %}
                {{ section.settings.cta_button | escape }}
              {% else %}
                {{ 'contact.cta_button' | t }}
              {% endif %}
            </a>
          </div>

          <p class="contact__footnote">
            {{ 'contact.footer_note' | t }}
          </p>
        </div>
      </aside>
    </div>
  </div>
</section>

{% stylesheet %}
  /* ===== Contact (scoped) ===== */
  .contact {
    --bf-red: #d61818;
    --bf-black: #000000;
    --bf-white: #ffffff;
    --bf-gray-100: #f4f4f5;
    --bf-gray-200: #e4e4e7;
    --bf-gray-700: #3f3f46;

    --bf-radius: 14px;
    --bf-shadow: 0 12px 30px rgba(0, 0, 0, 0.12);

    background: var(--bf-black);
    color: var(--bf-white);
  }

  /* Avoid site-wide body styling. Keep it scoped to the page container. */
  .contact .contact__inner {
    padding: clamp(1.25rem, 3vw, 2.5rem) var(--page-margin);
    max-width: 1200px;
    margin: 0 auto;
    display: grid;
    gap: clamp(1rem, 2.5vw, 1.75rem);
  }

  /* HERO */
  .contact__hero {
    display: grid;
    grid-template-columns: 1.4fr 1fr;
    gap: clamp(1rem, 2.5vw, 1.5rem);
    align-items: start;
  }

  .contact__eyebrow {
    margin: 0 0 .35rem;
    font-size: .85rem;
    letter-spacing: .08em;
    text-transform: uppercase;
    color: rgba(255,255,255,.7);
  }

  .contact__heading {
    margin: 0 0 .5rem;
    font-size: clamp(1.9rem, 3.5vw, 2.6rem);
    line-height: 1.05;
    letter-spacing: -0.02em;
    color: var(--bf-white);
  }

  .contact__subheading {
    margin: 0 0 1rem;
    font-size: 1.05rem;
    color: rgba(255,255,255,.82);
    max-width: 52ch;
  }

  .contact__hero-copy {
    display: grid;
    gap: .6rem;
    margin: 0 0 1.1rem;
    color: rgba(255,255,255,.86);
    max-width: 62ch;
  }

  .contact__stats {
    display: grid;
    grid-template-columns: repeat(3, minmax(0, 1fr));
    gap: .75rem;
    margin: 0 0 1.1rem;
  }

  .contact__stat {
    background: rgba(255,255,255,.06);
    border: 1px solid red;
    border-radius: var(--bf-radius);
    padding: .85rem .85rem;
  }

  .contact__stat-value {
    margin: 0;
    font-size: 1.55rem;
    font-weight: 800;
    letter-spacing: -0.02em;
  }

  .contact__stat-label {
    margin: .2rem 0 0;
    color: rgba(255,255,255,.75);
    font-size: .95rem;
  }

  .contact__quick-actions {
    display: flex;
    flex-wrap: wrap;
    gap: .6rem;
  }

  .contact__btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    padding: .65rem 1rem;
    border-radius: 999px;
    text-decoration: none;
    font-weight: 700;
    border: 1px solid rgba(255,255,255,.18);
    color: var(--bf-white);
    background: rgba(255,255,255,.08);
  }

  .contact__btn--primary {
    background: var(--bf-red);
    border-color: rgba(0,0,0,.15);
  }

  .contact__btn--ghost:hover {
    background: rgba(255,255,255,.12);
  }

  .contact__btn--primary:hover {
    filter: brightness(1.02);
  }

  /* Panels */
  .contact__panel {
    background: var(--bf-white);
    color: var(--bf-black);
    border-radius: var(--bf-radius);
    box-shadow: var(--bf-shadow);
    border: 1px solid var(--bf-gray-200);
    padding: 1.15rem 1.15rem;
  }

  .contact__card-title,
  .contact__panel-title {
    margin: 0 0 .35rem;
    font-size: 1.05rem;
    font-weight: 800;
    letter-spacing: -0.01em;
  }

  .contact__panel-subtitle {
    margin: 0 0 1rem;
    color: #111;
    opacity: .75;
    max-width: 70ch;
  }

  /* Hero right card */
  .contact__hero-right {
    background: var(--bf-white);
    color: var(--bf-black);
    border-radius: var(--bf-radius);
    box-shadow: var(--bf-shadow);
    border: 1px solid var(--bf-gray-200);
    padding: 1.15rem 1.15rem;
  }

  .contact__services {
    display: grid;
    gap: .75rem;
    margin-top: 1rem;
  }

  .contact__service {
    border: 1px solid var(--bf-gray-200);
    border-radius: 12px;
    padding: .85rem .85rem;
    background: var(--bf-gray-100);
  }

  .contact__service-title {
    margin: 0 0 .25rem;
    font-size: 1rem;
    font-weight: 800;
  }

  .contact__service-copy {
    margin: 0;
    color: rgba(0,0,0,.75);
  }

  .contact__mini {
    margin-top: 1rem;
    border-top: 1px solid var(--bf-gray-200);
    padding-top: .85rem;
    display: grid;
    gap: .5rem;
  }

  .contact__mini-row {
    display: grid;
    grid-template-columns: 140px 1fr;
    gap: .75rem;
    align-items: baseline;
    font-size: .95rem;
  }

  .contact__mini-label {
    color: rgba(0,0,0,.62);
    font-weight: 700;
  }

  .contact__mini-value a {
    color: var(--bf-black);
    text-decoration: underline;
    text-decoration-color: rgba(0,0,0,.25);
  }

  /* MAIN GRID */
  .contact__grid {
    display: grid;
    grid-template-columns: 1.35fr 1fr;
    gap: clamp(1rem, 2.5vw, 1.75rem);
    align-items: start;
  }

  .contact__details {
    display: grid;
    gap: 1rem;
  }

  .contact__extras {
    display: grid;
    gap: .75rem;
  }

  .contact__extra {
    display: grid;
    grid-template-columns: 44px 1fr;
    gap: .75rem;
    align-items: start;
    border: 1px solid var(--bf-gray-200);
    border-radius: 12px;
    padding: .85rem;
    background: var(--bf-gray-100);
  }

  .contact__icon {
    width: 44px;
    height: 44px;
    border-radius: 999px;
    background: var(--bf-red);
    display: grid;
    place-items: center;
    color: var(--bf-white);
  }

  .contact__icon svg {
    width: 20px;
    height: 20px;
    fill: currentColor;
  }

  .contact__extra-title {
    margin: 0 0 .2rem;
    font-size: 1rem;
    font-weight: 800;
  }

  .contact__extra-copy {
    margin: 0;
    color: rgba(0,0,0,.78);
  }

  .contact__muted {
    color: rgba(0,0,0,.55);
  }

  .contact__map iframe,
  .contact__map > iframe {
    width: 100%;
    border: 0;
    border-radius: 12px;
    min-height: 320px;
  }

  /* FAQ */
  .contact__faq {
    display: grid;
    gap: .65rem;
  }

  .contact__faq-item {
    border: 1px solid var(--bf-gray-200);
    border-radius: 12px;
    background: var(--bf-gray-100);
    padding: .75rem .85rem;
  }

  .contact__faq-item summary {
    cursor: pointer;
    font-weight: 800;
    list-style: none;
  }

  .contact__faq-item summary::-webkit-details-marker {
    display: none;
  }

  .contact__faq-answer {
    padding-top: .55rem;
    color: rgba(0,0,0,.75);
  }

  /* FORM */
  .contact__form {
    display: grid;
  }

  .contact__panel--sticky {
    position: sticky;
    top: 1rem;
  }

  .field {
    display: grid;
    gap: .35rem;
    margin-bottom: .75rem;
  }

  .field label {
    font-weight: 700;
  }

  .field input,
  .field textarea {
    width: 100%;
    padding: .7rem .85rem;
    border-radius: 12px;
    border: 1px solid black;
    background: var(--bf-white);
    color: var(--bf-black);
    outline: none;
  }

  .field input:focus,
  .field textarea:focus {
    border-color: rgba(214, 24, 24, .55);
    box-shadow: 0 0 0 4px rgba(214, 24, 24, .12);
  }

  .contact__hint {
    margin: .35rem 0 0;
    font-size: .9rem;
    color: rgba(0,0,0,.62);
  }

  .contact__submit {
    width: 100%;
    padding: .8rem 1rem;
    border-radius: 999px;
    border: none;
    background: var(--bf-red);
    color: var(--bf-white);
    font-weight: 800;
    cursor: pointer;
  }

  .contact__submit:hover {
    filter: brightness(1.02);
  }

  .contact__errors {
    margin: 0 0 .75rem;
    padding-left: 1.1rem;
    color: var(--bf-red);
  }

  .contact__success {
    margin: 0 0 .75rem;
    padding: .75rem .85rem;
    border-radius: 12px;
    background: rgba(214, 24, 24, .08);
    border: 1px solid rgba(214, 24, 24, .18);
    color: #111;
    font-weight: 700;
  }

  /* CTA */
  .contact__cta {
    margin-top: .9rem;
    border-radius: 12px;
    background: var(--bf-black);
    color: var(--bf-white);
    padding: 1rem 1rem;
    display: grid;
    grid-template-columns: 1fr auto;
    gap: 1rem;
    align-items: center;
  }

  .contact__cta-title {
    margin: 0;
    font-weight: 900;
    letter-spacing: -0.01em;
  }

  .contact__cta-subtitle {
    margin: .25rem 0 0;
    color: rgba(255,255,255,.78);
  }

  .contact__cta-btn {
    background: var(--bf-red);
    color: var(--bf-white);
    text-decoration: none;
    padding: .7rem 1.15rem;
    border-radius: 999px;
    font-weight: 900;
    white-space: nowrap;
  }

  .contact__footnote {
    margin: .9rem 0 0;
    font-size: .92rem;
    color: rgba(0,0,0,.62);
  }

  /* Responsive */
  @media (max-width: 980px) {
    .contact__hero {
      grid-template-columns: 1fr;
    }
    .contact__grid {
      grid-template-columns: 1fr;
    }
    .contact__panel--sticky {
      position: static;
    }
    .contact__mini-row {
      grid-template-columns: 1fr;
      gap: .15rem;
    }
  }

  @media (max-width: 520px) {
    .contact__stats {
      grid-template-columns: 1fr;
    }
    .contact__cta {
